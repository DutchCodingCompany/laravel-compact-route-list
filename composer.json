        "php" : ">=7.4",
        "laravel/framework": "^6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "DCC\\CompactRouteList\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DCC\\CompactRouteList\\CompactRouteListServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "license": "MIT",
    "authors": [
